<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<ExtractVariables async="false" continueOnError="false" enabled="true" name="EV-UserInfo">
    <DisplayName>EV-UserInfo</DisplayName>
    <Properties/>
