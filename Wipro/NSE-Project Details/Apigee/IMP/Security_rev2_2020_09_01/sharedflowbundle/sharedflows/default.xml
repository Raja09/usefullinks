<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<SharedFlow name="default">
    <Step>
        <Condition>(accesstoken.ip_address != proxy.client.ip)</Condition>
        <Name>OA-InvalidateToken</Name>
    </Step>
    <Step>
        <Name>OA-VerifyAccessToken</Name>
    </Step>
    <Step>
        <Name>SA-RequestThrottle</Name>
    </Step>
    <Step>
        <Name>RE-DefaultContentBasedAttacks</Name>
    </Step>
</SharedFlow>